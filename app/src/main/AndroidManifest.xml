<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="labs.consultations">
    <uses-feature android:name="android.hardware.camera"></uses-feature>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Consultations">
        <service android:name=".services.Messages"></service>
        <activity android:name=".lab5_activity" android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".lab4_activity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".lab3_activity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".lab1_activity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".lab2_activity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>